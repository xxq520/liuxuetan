.jia_bg .sendMessage{
  background-color: #fff;
  position: absolute;
  right: 0;
  top: 54rpx;
  width: 180rpx;
  height: 110rpx;
  z-index: 1000;
  border-radius: 8rpx;
  box-shadow: 0rpx 5rpx 18rpx rgba(0, 0, 0, .05);
}
.sendMessage image{
  width: 27rpx;
  height: 27rpx;
  margin-right: 12rpx;
  vertical-align: middle;
  margin-top: -5rpx;

}
.sendMessage view{
  height: 55rpx;
  line-height: 55rpx;
  text-align: center;
  font-size: 24rpx
}
.sendMessage:after {
  position: absolute;
  top: 50%;
  right: 8px;
  margin-top: -34px;
  border-color: transparent transparent #fff #fff;
  border-style: solid;
  border-width: 6px;
  -webkit-transform: rotate(-75deg);
  transform: rotate(-85deg);
  border-radius: 22rpx;
  opacity: 1;
  content: "";
}
.van-transition{
  background: transparent !important
}
