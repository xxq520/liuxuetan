
page{
  background: #f5f5f5;
  height: 100%;
}
.top_search {
  height: 133rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  display: flex;
  align-items: center;
}

.top_bg {
  background-color: #fff;
}

.logo-box {
  width: 154rpx;
  display: flex;
  justify-content: center;
}

.logo {
  width: 109rpx;
  height: 59rpx;
}

.search-box {
  width: 470rpx;
  height: 60rpx;
  background-color: #ffffff;
  border-radius: 30rpx;
  position: relative;
  padding: 0 28rpx 0 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.search-box input {
  font-size: 24rpx;
  color: #8c8c8c;
}

.search_btn {
  width: 28rpx;
  height: 32rpx;

}

.right_jia {
  width: 126rpx;
  display: flex;
  justify-content: center;
}

.jia_bg {
  width: 40rpx;
  height: 40rpx;
  background-color: #e9a431;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
}

.jia_bg image {
  width: 17rpx;
  height: 17rpx;
}

.tab_bar {
  height: 73rpx;
  display: flex;
  justify-content: space-around;
  align-items: flex-start;
  padding: 0 30%;
  background: #fff;
  margin-bottom: 19rpx;
}

.tab_item {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #e9a431;

}

.bor_b::before {
  content: "";
  width: 59rpx;
  height: 6rpx;
  font-weight: 600;
  background: #e9a431;
  position: absolute;
  bottom: -12rpx;
  left: 50%;
  transform: translateX(-50%)
}

.current_color {
  color: #333333 !important;
}

.xiala {
  width: 17rpx;
  height: 10rpx;
  margin-left: 15rpx;
}

.article_item {
  width: 94%;
  padding: 26rpx 20rpx 30rpx 20rpx;
  display: flex;
  margin: 0 auto;
  flex-direction: column;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.article_title {
  display: flex;
  margin-bottom: 10rpx;
  justify-content: space-between;
}

.title_txt {
  font-size: 28rpx;
  color: #e9a431;
}

.collect_btn {
  width: 100rpx;
  height: 40rpx;
  border-radius: 19rpx;
  border: solid 1rpx #e9a431;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 11.5rpx 18rpx;
  font-size: 18rpx;
  letter-spacing: 1rpx;
  color: #e9a431;

}

.collect_btn image {
  width: 19rpx;
  height: 19rpx;
}

.article_context {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 5;
  overflow: hidden;
  font-size: 24rpx;
	line-height: 1.8;
	letter-spacing: 0rpx;
	color: #333333;
}
.full_text{
  font-size: 24rpx;
	letter-spacing: 1rpx;
	color: #e9a431;
}
.user_info{
  display: flex;
  align-items: center;
  margin-top: 30rpx;
  padding: 0 20rpx;
}
.user_tx{
	width: 56rpx;
	height: 56rpx;
}
.user_name{
  font-size: 22rpx;
	letter-spacing: 1rpx;
	color: #333333;
}
.top_info{
  display: flex;
  align-items: center;
}
.publish_time{
  font-size: 16rpx;
	letter-spacing: 1rpx;
  color: #8c8c8c;
  margin-left: 15rpx;
}
.title_box{
  display: flex;
  align-items: center;
}
.wen_ti{
  width: 28rpx;
	height: 25rpx;
	background-color: #e9a431;
  border-radius: 4rpx;
  font-size: 18rpx;
	line-height: 18rpx;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 20rpx;
}
.info_content{
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
}
.browse{
  display: flex;
  align-items: center;
}
.bottom_info{
  display: flex;
  align-items: center;
  margin-top: 5rpx;
}
.browse image{
  width: 22rpx;
	height: 13rpx;
}
.browse_context{
margin-left: 16rpx;
}
.browse_context,.browse_num{
  font-size: 18rpx;
	letter-spacing: 1rpx;
	color: #8c8c8c;
}
.browse_num{
  margin-left: 14rpx;
}
.attention{
  display: flex;
  margin-left: 45rpx;
  align-items: center;
}
.attention image{
  width: 20rpx;
	height: 19rpx;
}
.attention_btn{
  font-size: 18rpx;
  color: #8c8c8c;
  margin-left: 8rpx;
}
.status{
  display: flex;
  align-items: center;
  margin-top: 28rpx;
}
.status .status_item:nth-child(1) image{
  width: 24rpx;
	height: 17rpx;
}
.status .status_item:nth-child(2) image{
  width: 21rpx;
	height: 18rpx;
}
.status .status_item:nth-child(3) image{
  width: 21rpx;
	height: 19rpx;
}
.status_item{
  width: 33.33333333%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
	letter-spacing: 1rpx;
	color: #8c8c8c;
}
.status_item text{
  margin-left: 20rpx;
}
.status_item:nth-child(2){
  border-left: solid 1rpx #dddddd;
  border-right: solid 1rpx #dddddd;
}
.zan_color{
  color: #a6d9e1;
}
.wen_da{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 24rpx;
	line-height: 1.8;
	letter-spacing: 0rpx;
	color: #333333;
}
.mar_top_16{
margin-top: 16rpx !important;
}
.mar_b_36{
  margin-bottom: 36rpx;
}
.wen_title_cor{
	color: #333333;
}