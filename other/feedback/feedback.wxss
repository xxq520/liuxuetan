/* pages/feedback/feedback.wxss */
.feedbackBody{
  width: 100%;
	height: 1192rpx;
	background-color: #ffffff;
  border-top: 1rpx solid #dddddd;
  position: relative;
  padding-top: 31rpx;
}
.feedbackBody .content{
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #333;
  /* margin-top: 31rpx; */
  /* width: 90%; */
  /* height: auto; */
  /* margin: auto; */
  margin-left: 29rpx;
}
.box{
   position: absolute;
  top: 395rpx;
  left: 30rpx;
  display: flex;
}
.img{
  width: 60rpx;
	height: 53rpx;
  
}
.frame{
  width: 165rpx;
	height: 165rpx;
  border-radius: 10rpx;
  border: solid 1rpx #dddddd;
  text-align: center;
  line-height: 165rpx;
  margin-right: 19rpx;
  margin-bottom: 130rpx;
}
.camera{
  /* display: none; */
  position: relative;
}
.delete{
  width: 30rpx !important;
	height: 30rpx !important;
  position: absolute;
 right: -15rpx;
 top: -15rpx;
  opacity: 0.8;
}
.camera image{
  width: 165rpx;
	height: 165rpx;
}
.commit{
  position: absolute;
  top: 32%;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 90% !important;
  height: 85rpx;
  /* text-align: center; */
  font-size: 34rpx;
  color: #ffffff;
  background-color: #3b5370;
	border-radius: 10rpx;
  display: block;
}
.van-uploader__upload {
  background: transparent !important;
   border: solid 1rpx #dddddd;
}