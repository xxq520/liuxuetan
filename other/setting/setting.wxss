/* other/withdrawal/withdrawal.wxss */
page {
    background-color: #f5f5f5;
}
.content{
    margin: 35rpx 0;
}
.contentItem{
    padding: 0 29rpx;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: solid 1rpx rgba(221, 221, 221, .8);
    background-color: #fff;
    align-items: center;
}
.contentItem text{
    font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #333333;
}
.contentItem image{
    width: 15rpx;
	height: 25rpx;
}
.exit{
    height: 88rpx;
    background-color: #ffffff;
    line-height: 88rpx;
    text-align: center;
    font-family: PingFang-SC-Medium;
	font-size: 32rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #ff301e;
}
